{
  "name": "server-express",
  "version": "1.0.0",
  "main": "src/server.ts",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "rm -rf dist/ && tsc -p .",
    "db:seed": "NODE_ENV=development ts-node -r tsconfig-paths/register src/database/seeder/index.ts",
    "start": "NODE_ENV=production ts-node -r tsconfig-paths/register dist/server.js",
    "test": "NODE_ENV=test jest --forceExit --detectOpenHandles  --watchAll --maxWorkers=1"
  },
  "license": "MIT",
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.11.10",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb-memory-server": "^9.2.0",
    "mongoose": "^8.3.5",
    "nodemon": "^3.1.0",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.7.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "validator": "^13.12.0"
  }
}
